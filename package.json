{
  "name": "meetzen",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "envs": "turbo envs",
    "dev": "turbo dev",
    "lint": "oxlint",
    "check-types": "turbo check-types",
    "shadcn": "bun --filter @meetzen/ui shadcn",
    "prisma": "bun --filter @meetzen/database prisma-command",
    "clean-workspaces": "turbo clean-workspace && rm -rf node_modules"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.12",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@elysiajs/eden": "^1.3.2",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@tabler/icons-react": "^3.33.0",
    "@tanstack/react-query": "^5.80.2",
    "better-auth": "^1.2.8",
    "elysia": "^1.3.3",
    "framer-motion": "^12.15.0",
    "next-themes": "^0.4.6",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.0",
    "temporal-polyfill": "^0.3.0",
    "tw-animate-css": "^1.3.2"
  }
